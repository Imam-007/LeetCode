class Solution {
    public String restoreString(String s, int[] in) {
        char arr[]=new char[s.length()];
        for(int i=0;i<in.length;i++){
            arr[in[i]]=s.charAt(i);
        }
        String str="";
        for(int i=0;i<arr.length;i++){
            str+=arr[i];
        }
        return str;
    }
}
